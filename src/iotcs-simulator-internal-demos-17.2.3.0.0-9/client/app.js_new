    // Determine where to start chart scale and do our best to accommodate ranges of all numeric attributes.
    // scale these ranges to 100. Values measured in the same units are scaled using the same factor.
    $scope.getStartValue = function(dm) {
      $scope.factorsByUnits = {};
      var maxRangeByUnits = {};
      var minValue = 0;
      dm.chartableAttributes.forEach(function(attr) {
        if ((attr.type === 'INTEGER' || attr.type === 'NUMBER') && attr.range) {
          var rangeValues = attr.range.split(',');
          var range = Math.max(Math.abs(rangeValues[1]), Math.abs(rangeValues[0]));
          if (maxRangeByUnits[attr.units]) {
            if (range > maxRangeByUnits[attr.units]) {
              maxRangeByUnits[attr.units] = range;
              $scope.factorsByUnits[attr.units] = Math.pow(10, Math.floor(Math.log10(100.0 / range)));
            }
          } else {
            maxRangeByUnits[attr.units] = range;
            $scope.factorsByUnits[attr.units] = Math.pow(10, Math.floor(Math.log10(100.0 / range)));
          }
          minValue = Math.min(minValue, rangeValues[0] * $scope.factorsByUnits[attr.units]);
        }
      });

      return Math.round(minValue / 10.0) * 10.0 - 10.0;
    };

    $scope.chartLabels = [];

    $scope.chartOptions = {
      pointDot: false,
      datasetFill: false,
      animation: false,
      // scaleOverride: true,
      // scaleSteps: 12,
      // scaleStepWidth: 10,
      scaleLineColor: '#303d43',
      scaleGridLineColor: '#303d43',
      scaleFontColor: '#8d9473',
      scaleShowVerticalLines: false,
      scaleFontFamily: "'Dosis', 'Helvetica Neue', 'Helvetica', 'Arial', sans-serif",
      bezierCurveTension: 0.6,
      showTooltips: false
    };

    $scope.$watch('device.dm', function(dm) {
      $scope.chartLabels = [];
      $scope.chartSeries = [];
      $scope.chartData = [];
      if (dm) {
        if (!$scope.chartOptions.scaleStartValue) {
          $scope.chartOptions.scaleStartValue = $scope.getStartValue(dm);
        }
        for (var i = 0; i < dm.chartableAttributes.length; i++) {
          var attr = dm.chartableAttributes[i];
          if (attr.type === 'NUMBER' || attr.type === 'INTEGER') {
            $scope.chartData.push([]);
            if ($scope.factorsByUnits[attr.units] !== 1) {
              $scope.chartSeries.push(attr.displayName + ' * ' + $scope.factorsByUnits[attr.units] +
                ' [' + attr.units + ']');
            } else {
              $scope.chartSeries.push(attr.displayName + ' [' + attr.units + ']');
            }
          }
        }
      }
    });

    $scope.$watch('device.stateImage', function() {
      if ($scope.alertImageInEffect) {
        // Keep alert image till it's in effect
        $scope.device.stateImage = $scope.device.alertImage;
      } else if ($scope.device.stateImage && $scope.device.stateImage.isSprite) {
        $scope.animFrame = 0;
        animation = $interval(function() {
          if ($scope.device && $scope.device.dm && $scope.device.isOn) {
            if ($scope.animFrame < $scope.device.dm.images[0].framesNumber - 1) {
              $scope.animFrame++;
            } else {
              $scope.animFrame = 0;
            }
          }
        }, 30);
      } else {
        if (animation) {
          $interval.cancel(animation);
        }
      }
      $scope.updateImageSize();
    });

    $scope.$watch('device.controls', function() {
      if ($scope.device.controls) {
        $scope.device.controls.forEach(function(control) {
          var formControl;
          for (var i = 0; i < $scope.controls.length; i++) {
            if ($scope.controls[i].name === control.name) {
              formControl = $scope.controls[i];
            }
          }
          formControl.value = control.value;
        });
      }
      if ($scope.device.monitors) {
        $scope.device.monitors.forEach(function(monitor) {
          var formMonitor;
          for (var i = 0; i < $scope.monitors.length; i++) {
            if ($scope.monitors[i].name === monitor.name) {
              formMonitor = $scope.monitors[i];
            }
          }

          formMonitor.value = monitor.value;
        });
      }
    });

    $scope.$watch('device.time', function(newTime) {
      if (!$scope.deviceTabActive) {
        $scope.chartLabels.push($filter('date')(newTime, 'h:mm:ss'));

        var index = 0;
        $scope.device.dm.chartableAttributes.forEach(function(attr) {
          if (attr.type === 'NUMBER' || attr.type === 'INTEGER') {
            if (attr.range) {
              var value = $scope.device[attr.name] * $scope.factorsByUnits[attr.units];
              $scope.chartData[index].push(value); //(value-range[0])*100.0/(range[1]-range[0]));
              index++;
            }
          }
        });
        if ($scope.chartLabels.length > 30) {
          $scope.chartLabels.shift();
          for (var prop in $scope.chartData) {
            if (!$scope.chartData.hasOwnProperty(prop)) {
              continue;
            }
            $scope.chartData[prop].shift();
          }
        }
      }
    });

    $scope.$on('$destroy', function() {
      if (animation) {
        $interval.cancel(animation);
      }
      $(window).off('resize.doResize'); //remove the handler added earlier
    });

    $scope.destroyDevice = function() {
      $scope.$emit('destroyDevice', $scope.device);
      $scope.device = null;
    };
  }]);

}());

