   <div class=\"cell switch\">\n        <!--toggle-switch title=\"Interactive\" ng-model=\"attr.visible\"></toggle-switch-->\n        <div class=\"simple-onoffswitch\" title=\"Interactive\" tabindex=\"0\">\n          <input type=\"checkbox\" name=\"attribute-visible\" class=\"onoffswitch-checkbox\"\n                 id=\"attribute_visible_{{attr.name}}\" ng-model=\"attr.visible\" ng-true-value=\"false\" ng-false-value=\"true\">\n          <label class=\"onoffswitch-label\" for=\"attribute_visible_{{attr.name}}\">\n            <span class=\"onoffswitch-inner\"></span>\n            <span class=\"onoffswitch-switch\"></span>\n          </label>\n        </div>\n      </div>\n      <div class=\"cell display-name\"><input type=\"text\" class=\"input form-control\" id=\"displayName\" title=\"Display Name\" ng-model=\"attr.displayName\" ng-disabled=\"!attr.chartable && !attr.visible\"></div>\n      <div class=\"cell switch\">\n        <!--toggle-switch title=\"Chartable\" ng-model=\"attr.chartable\"></toggle-switch-->\n        <div class=\"simple-onoffswitch\" title=\"Chartable\" tabindex=\"0\">\n          <input type=\"checkbox\" name=\"attribute-visible\" class=\"onoffswitch-checkbox\"\n                 id=\"attribute_chartable_{{attr.name}}\" ng-model=\"attr.chartable\" ng-true-value=\"false\" ng-false-value=\"true\">\n          <label class=\"onoffswitch-label\" for=\"attribute_chartable_{{attr.name}}\">\n            <span class=\"onoffswitch-inner\"></span>\n            <span class=\"onoffswitch-switch\"></span>\n          </label>\n        </div>\n\n      </div>\n      <div class=\"cell units\"><input type=\"text\" class=\"input form-control\" id=\"units\" title=\"Units\" ng-model=\"attr.units\" ng-disabled=\"!attr.chartable && !attr.visible\"></div>\n      <div class=\"cell format\"><input type=\"text\" class=\"input form-control\" id=\"format\" title=\"Format\" ng-model=\"attr.format\" ng-disabled=\"!attr.visible\"></div>\n      <div class=\"cell\"><input type=\"text\" class=\"input form-control\" id=\"initialValue\" title=\"Initial Value\" ng-model=\"attr.initialValue\" ng-class=\"checkForErrors(attr, \'initialValue\')\"></div>\n      <div class=\"cell function\">\n        <select class=\"form-control input\"\n                aria-describedby=\"onFunctionLabel\"\n                title=\"Function\"\n                ng-model=\"attr.onFunction\"\n                focus-me=\"true\"\n                ng-class=\"checkForErrors(attr, \'onFunction\')\">\n          <option value=\"\">\n          <option ng-repeat=\"f in functions\" value=\"{{f.name}}\" ng-selected=\"f.name===attr.onFunction\">\n              {{f.name.indexOf(\"$sharedFunctions.\")===0 ? f.name.substring(17) : f.name}}\n          </option>\n        </select>\n\n        <div class=\"arg-line\"  ng-repeat=\"arg in getArguments(functions, attr.onFunction)\">\n          <div class=\"cell arg-name\">\n            {{arg.name}}\n          </div>\n          <div ng-if=\"arg.type===\'FILE\'\">\n            <label class=\"btn btn-default\" title=\"Upload\" for=\"uploadFile_{{dmIndex}}_{{attrIndex}}_onArguments_{{arg.name}}\">\n              <input id=\"uploadFile_{{dmIndex}}_{{attrIndex}}_onArguments_{{arg.name}}\" type=\"file\" style=\"display:none;\"  on-file-change=\"uploadDataFile\" ng-model=\"attr.onArguments[arg.name]\"><span class=\"glyphicon glyphicon-cloud-upload\" aria-hidden=\"true\"></span>\n            </label>\n          </div>\n          <div class=\"cell arg-value\">\n           <input type=\"text\" class=\"input form-control\" id=\"arg\" title=\"{{arg.name}}\" ng-disabled=\"arg.readonly\"\n                  ng-init=\"attr.onArguments[arg.name] = (attr.onArguments[arg.name] || attr.onArguments[arg.name] === 0\n                  || attr.onArguments[arg.name] === false) ? attr.onArguments[arg.name] : arg.default\"\n                  ng-model=\"attr.onArguments[arg.name]\" ng-class=\"checkForErrors(attr.onArguments, arg.name)\">\n          </div>\n        </div>\n\n      </div>\n\n      <div class=\"cell off-function\">\n        <select class=\"form-control input\"\n                aria-describedby=\"offFunctionLabel\"\n                title=\"Function\"\n                ng-model=\"attr.offFunction\"\n                focus-me=\"true\"\n                ng-class=\"checkForErrors(attr, \'offFunction\')\">\n          <option value=\"\">\n          <option ng-repeat=\"f in functions\" value=\"{{f.name}}\" ng-selected=\"f.name===attr.offFunction\">\n              {{f.name.indexOf(\"$sharedFunctions.\")===0 ? f.name.substring(17) : f.name}}\n          </option>\n        </select>\n        <div class=\"arg-line\"  ng-repeat=\"arg in getArguments(functions, attr.offFunction)\">\n          <div class=\"cell arg-name\">\n            {{arg.name}}\n          </div>\n          <div ng-if=\"arg.type===\'FILE\'\">\n            <label class=\"btn btn-default\" title=\"Upload\" for=\"uploadFile_{{dmIndex}}_{{attrIndex}}_offArguments_{{arg.name}}\">\n              <input id=\"uploadFile_{{dmIndex}}_{{attrIndex}}_offArguments_{{arg.name}}\" type=\"file\" style=\"display:none;\" on-file-change=\"uploadDataFile\" ng-model=\"attr.offArguments[arg.name]\"><span class=\"glyphicon glyphicon-cloud-upload\" aria-hidden=\"true\"></span>\n            </label>\n          </div>\n          <div class=\"cell arg-value\">\n           <input type=\"text\" class=\"input form-control\" id=\"arg\" title=\"{{arg.name}}\"  ng-disabled=\"arg.readonly\"\n                  ng-init=\"attr.offArguments[arg.name] = (attr.offArguments[arg.name] || attr.offArguments[arg.name] === 0\n                  || attr.offArguments[arg.name] === false) ? attr.offArguments[arg.name] : arg.default\"\n                  ng-model=\"attr.offArguments[arg.name]\" ng-class=\"checkForErrors(attr.offArguments, arg.name)\">\n          </div>\n        </div>\n      </div>\n    </div>\n\n  <!---------------------- Alerts ---------------------------->\n\n    <div class=\"table-line subsection\" ng-if=\"deviceModel.formats.length\">\n      <div class=\"cell title\">Alerts</div>\n    </div>\n\n    <div class=\"table-line header\" ng-if=\"deviceModel.formats.length\">\n      <div class=\"cell identifier\">urn</div>\n      <div class=\"cell switch\">Interactive</div>\n      <div class=\"cell\">Display Name</div>\n      <div class=\"cell function\">Condition</div>\n      <div class=\"cell arguments\">Arguments</div>\n      <div class=\"cell arguments\">Values</div>\n      <div class=\"cell select-image\" id=\"alertImageLabel\">Image</div>\n      <div class=\"cell\">Image Time (ms)</div>\n    </div>\n    <div class=\"table-line\" ng-repeat=\"format in deviceModel.formats\">\n      <div class=\"cell identifier\" title=\"{{format.urn}}\">{{format.urn}}</div>\n      <div class=\"cell switch\">\n        <!--toggle-switch title=\"Interactive\" ng-model=\"format.visible\"></toggle-switch-->\n        <div class=\"simple-onoffswitch\" title=\"Interactive\" tabindex=\"0\">\n          <input type=\"checkbox\" name=\"attribute-visible\" class=\"onoffswitch-checkbox\"\n                 id=\"format_visible_{{format.urn}}\" ng-model=\"format.visible\" ng-true-value=\"false\" ng-false-value=\"true\">\n          <label class=\"onoffswitch-label\" for=\"format_visible_{{format.urn}}\">\n            <span class=\"onoffswitch-inner\"></span>\n            <span class=\"onoffswitch-switch\"></span>\n          </label>\n        </div>\n\n      </div>\n      <div class=\"cell\"><input type=\"text\" class=\"input form-control\" id=\"formatDisplayName\" title=\"Display Name\" ng-model=\"format.displayName\" ng-disabled=\"!format.visible\"></div>\n      <div class=\"cell function\">\n        <select class=\"form-control input\"\n                aria-describedby=\"conditionLabel\"\n                title=\"Condition\"\n                ng-model=\"format.condition\"\n                focus-me=\"true\"\n                ng-disabled=\"format.visible\"\n                ng-class=\"checkForErrors(format, \'condition\')\">\n          <option value=\"\">\n          <option ng-repeat=\"f in conditions\" value=\"{{f.name}}\" ng-selected=\"f.name===format.condition\">\n              {{f.name.indexOf(\"$sharedFunctions.\")===0 ? f.name.substring(17) : f.name}}\n          </option>\n        </select>\n      </div>\n      <div class=\"cell arguments\">\n        <div class=\"arg-line\"  ng-repeat=\"arg in getArguments(conditions, format.condition)\">\n          <div class=\"cell arg-name\">\n            {{arg.name}}\n          </div>\n          <div class=\"cell arg-value\">\n            <input type=\"text\" class=\"input form-control\" id=\"formatArg\" title=\"{{arg.name}}\" ng-disabled=\"arg.readonly\"\n                   ng-init=\"format.conditionArguments[arg.name] = (format.conditionArguments[arg.name] ||\n                   format.conditionArguments[arg.name] === false || format.conditionArguments[arg.name] === 0) ? format.conditionArguments[arg.name] : arg.default\"\n                   ng-model=\"format.conditionArguments[arg.name]\" ng-disabled=\"format.visible\" ng-class=\"checkForErrors(format.conditionArguments, arg.name)\">\n          </div>\n        </div>\n      </div>\n      <div class=\"cell arguments\">\n        <div class=\"arg-line\"  ng-repeat=\"field in format.value.fields\">\n          <div class=\"cell arg-name\">\n            {{field.name}}\n          </div>\n          <div class=\"cell arg-value\">\n            <input type=\"text\" class=\"input form-control\" id=\"fieldValue\" title=\"{{field.name}}\" ng-model=\"field.value\" ng-class=\"checkForErrors(field, \'value\')\">\n          </div>\n        </div>\n      </div>\n      <div class=\"cell select-image\">\n        <select class=\"form-control input\"\n                aria-describedby=\"alertImageLabel\"\n                title=\"Image\"\n                ng-model=\"format.image\"\n                focus-me=\"true\" >\n          <option ng-repeat=\"im in simulatorModel.images\" value=\"{{im.name}}\" ng-selected=\"im.name===format.image\">\n            {{im.name}}\n          </option>\n        </select>\n      </div>\n      <div class=\"cell\">\n        <input type=\"text\" class=\"input form-control\" id=\"imageTime\" title=\"Image Time\" ng-model=\"format.imageTime\">\n      </div>\n\n    </div>\n\n  <!---------------------- Actions ---------------------------->\n\n    <div class=\"table-line subsection\" ng-if=\"deviceModel.actions.length\">\n      <div class=\"cell title\">Actions</div>\n    </div>\n\n    <div class=\"table-line header\"  ng-if=\"deviceModel.actions.length\">\n      <div class=\"cell identifier\">Name</div>\n      <div class=\"cell\">Value</div>\n      <div class=\"cell function\">Function</div>\n      <div class=\"cell arguments\">Arguments</div>\n    </div>\n\n    <div class=\"table-line\" ng-repeat=\"action in deviceModel.actions\">\n      <div class=\"cell identifier\">{{action.name}}</div>\n      <div class=\"cell\"><input type=\"text\" class=\"input form-control\" id=\"actionValue\" title=\"Value\" ng-model=\"action.value\"></div>\n      <div class=\"cell function\">\n        <select class=\"form-control input\"\n                aria-describedby=\"functionLabel\"\n                title=\"Handler\"\n                ng-model=\"action.actionHandler\"\n                focus-me=\"true\"\n                ng-class=\"checkForErrors(action, \'actionHandler\')\">\n          <option value=\"\">\n          <option ng-repeat=\"f in handlers\" value=\"{{f.name}}\" ng-selected=\"f.name===action.actionHandler\">\n              {{f.name.indexOf(\"$sharedFunctions.\")===0 ? f.name.substring(17) : f.name}}\n          </option>\n        </select>\n      </div>\n      <div class=\"cell arguments\">\n        <div class=\"arg-line\"  ng-repeat=\"arg in getArguments(handlers, action.actionHandler)\">\n          <div class=\"cell arg-name\">\n            {{arg.name}}\n          </div>\n          <div class=\"cell arg-value\">\n            <input type=\"text\" class=\"input form-control\" id=\"actionArg\" title=\"{{arg.name}}\" ng-disabled=\"arg.readonly\"\n                   ng-init=\"action.actionArguments[arg.name] = (action.actionArguments[arg.name] || action.actionArguments[arg.name] === 0\n                   || action.actionArguments[arg.name] === false) ? action.actionArguments[arg.name] : arg.default\"\n                   ng-model=\"action.actionArguments[arg.name]\" ng-class=\"checkForErrors(action.actionArguments, arg.name)\">\n          </div>\n        </div>\n      </div>\n\n    </div>\n\n  </div>  <!-- deviceModel -->\n\n  <!---------------------- Events ---------------------------->\n\n  <div class=\"table-line section\">\n    <div class=\"cell title\">Events</div>\n    <div class=\"cell button-row\"></div>\n    <div class=\"cell add-button\"><button class=\"btn btn-default\" title=\"Add New Event\" ng-click=\"addEvent()\"><span class=\"glyphicon glyphicon-plus-sign\" aria-hidden=\"true\"></span> Add New Event</span></button></div>\n  </div>\n\n  <div class=\"table-line header\" ng-if=\"simulatorModel.events.length\">\n    <div class=\"cell identifier\">Name</div>\n    <div class=\"cell description\">Description</div>\n    <div class=\"cell select-image\" id=\"eventImageLabel\">Image</div>\n    <div class=\"cell preview\"></div>\n    <div class=\"cell clone-event\"></div>\n    <div class=\"cell delete-event\"></div>\n    <div class=\"cell add-attribute\"></div>\n  </div>\n  <div ng-repeat=\"event in simulatorModel.events track by $index\">\n\n    <div class=\"table-line\">\n      <div class=\"cell identifier\"><input type=\"text\" class=\"input form-control\" id=\"eventName\" title=\"Name\" ng-model=\"event.name\" ng-class=\"checkForErrors(event, \'name\')\"></div>\n      <div class=\"cell description\"><input type=\"text\" class=\"input form-control\" id=\"eventDescription\" title=\"Description\" ng-model=\"event.description\"></div>\n      <div class=\"cell select-image\">\n        <select class=\"form-control input\"\n              aria-describedby=\"eventImageLabel\"\n              title=\"Image\"\n              ng-model=\"event.image\"\n              focus-me=\"true\" >\n          <option ng-repeat=\"im in simulatorModel.images\" value=\"{{im.name}}\" ng-selected=\"im.name===event.image\">\n              {{im.name}}\n          </option>\n        </select>\n      </div>\n      <div class=\"cell preview\"><img class=\"img\" ng-src=\"{{getImageURL(event.image)}}\"></img></div>\n      <div class=\"cell clone-event\"><button class=\"btn btn-default\" title=\"Clone\" ng-click=\"cloneEvent($index)\"><span class=\"glyphicon glyphicon-duplicate\" aria-hidden=\"true\"></span> Clone</button></div>\n      <div class=\"cell delete-event\"><button class=\"btn btn-default\" title=\"Remove\" ng-click=\"deleteEvent($index)\"><span class=\"glyphicon glyphicon-remove-sign\" aria-hidden=\"true\"></span> Remove</button></div>\n      <div class=\"cell add-attribute\"><button class=\"btn btn-default\" title=\"Add Attribute\" ng-click=\"addAttribute(event)\"><span class=\"glyphicon glyphicon-plus-sign\" aria-hidden=\"true\"></span> Add Attribute</button></div>\n\n    </div>\n\n    <div class=\"table-line header indent\" ng-if=\"objectSize(event.attributes)>0\">\n      <div class=\"cell identifier\">Attribute</div>\n      <div class=\"cell function\">Function</div>\n      <div class=\"cell preview\">Arguments</div>\n      <div class=\"cell delete-attribute\"></div>\n    </div>\n\n    <div class=\"table-line indent\" ng-class=\"name.indexOf(\'_new_attribute\')===0 ? \'inprogress\' : \'\'\" ng-repeat=\"(name,attr) in event.attributes\">\n      <div class=\"cell identifier\" ng-if=\"name.indexOf(\'_new_attribute\')!==0\">{{name.substring(name.indexOf(\'.\') + 1)}}</div>\n      <div class=\"cell identifier\" ng-if=\"name.indexOf(\'_new_attribute\')===0\">\n        <select class=\"form-control input\"\n              aria-describedby=\"eventAttributeLabel\"\n              title=\"Event Attribute\"\n              ng-model=\"attr.selectedName\"\n              ng-change=\"setAttributeName(event, name, attr.selectedName)\"\n              focus-me=\"true\"\n              ng-class=\"checkForErrors(name, null)\">\n          <option value=\"\">- select attribute -</option>\n          <optgroup label=\"{{dm.urn}}\" ng-repeat=\"dm in simulatorModel.deviceModelsExtensions\">\n            <option ng-repeat=\"a in dm.attributes\" ng-value=\"dm.urn+\'.\'+a.name\">{{a.name}}</option>\n          </optgroup>\n        </select>\n      </div>\n      <div class=\"cell function\">\n        <select class=\"form-control input\"\n              aria-describedby=\"eventFunctionLabel\"\n              title=\"Function\"\n              ng-model=\"attr.onEventFunction\"\n              focus-me=\"true\"\n              ng-class=\"checkForErrors(attr, \'onEventFunction\')\">\n          <option value=\"\">\n          <option ng-repeat=\"f in functions\" value=\"{{f.name}}\" ng-selected=\"f.name===event.attributes[attr].onEventFunction\">\n              {{f.name.indexOf(\"$sharedFunctions.\")===0 ? f.name.substring(17) : f.name}}\n          </option>\n        </select>\n      </div>\n      <div class=\"cell arguments\">\n        <div class=\"arg-line\"  ng-repeat=\"arg in getArguments(functions, attr.onEventFunction)\">\n          <div class=\"cell arg-name\">\n            {{arg.name}}\n          </div>\n          <div ng-if=\"arg.type===\'FILE\'\">\n            <label class=\"btn btn-default\" title=\"Upload\" for=\"uploadFile_{{name}}_onEventArguments_{{arg.name}}\">\n              <input id=\"uploadFile_{{name}}_onEventArguments_{{arg.name}}\" type=\"file\" style=\"display:none;\" on-file-change=\"uploadDataFile\" ng-model=\"attr.onEventArguments[arg.name]\"><span class=\"glyphicon glyphicon-cloud-upload\" aria-hidden=\"true\"></span>\n            </label>\n          </div>\n\n          <div class=\"cell arg-value\">\n            <input type=\"text\" class=\"input form-control\" id=\"eventArg\" title=\"{{arg.name}}\" ng-disabled=\"arg.readonly\"\n                   ng-init=\"attr.onEventArguments[arg.name] = (attr.onEventArguments[arg.name] || attr.onEventArguments[arg.name] === 0\n                   || attr.onEventArguments[arg.name] === false) ? attr.onEventArguments[arg.name] : arg.default\"\n                   ng-model=\"attr.onEventArguments[arg.name]\" ng-class=\"checkForErrors(attr.onEventArguments, arg.name)\">\n          </div>\n        </div>\n      </div>\n\n      <div class=\"cell delete-attribute\"><button class=\"btn btn-default\" title=\"Remove Attribute\" ng-click=\"deleteAttribute(event, name)\"><span class=\"glyphicon glyphicon-remove-sign\" aria-hidden=\"true\"></span> Remove</button></div>\n    </div>\n    <div class=\"cell button-line\"></div>\n  </div>\n\n  <!---------------------- Images ---------------------------->\n\n  <div class=\"table-line section\">\n    <div class=\"cell title\">Images</div>\n    <div class=\"cell button-row\"></div>\n    <div class=\"cell add-button\"><button class=\"btn btn-default\" title=\"Add New Image\" ng-click=\"addImage()\"><span class=\"glyphicon glyphicon-plus-sign\" aria-hidden=\"true\"></span> Add New Image</span></button></div>\n  </div>\n\n  <div class=\"table-line header\" ng-if=\"simulatorModel.images.length\">\n    <div class=\"cell identifier\">Name</div>\n    <!--div class=\"cell file\">File</div-->\n    <div class=\"cell preview\">Preview</div>\n    <div class=\"cell\">Width</div>\n    <div class=\"cell\">Height</div>\n    <!--div class=\"cell\">Animated</div>\n    <div class=\"cell frames\">Number of Frames</div-->\n    <div class=\"cell file\">File</div>\n    <div class=\"cell file-upload\"></div>\n    <div class=\"cell\">Type</div>\n    <div class=\"cell delete-image\"></div>\n  </div>\n\n  <div class=\"table-line\" ng-repeat=\"image in simulatorModel.images\">\n    <div class=\"cell identifier\"><input type=\"text\" class=\"input form-control\" id=\"imageName\" title=\"Name\" ng-model=\"image.name\"></div>\n    <div class=\"cell preview\">\n      <div ng-if=\"image.type && image.type.indexOf(\'video\') === 0\">\n        <video class=\"img\" autoplay loop ng-src=\"{{getImageURLByName(image.file)}}\" type=\"{{image.type}}\">\n        </video>\n      </div>\n      <div ng-if=\"!image.type || (image.type != \'css_animation\' && (image.type.indexOf(\'video\') !== 0))\">\n        <img class=\"img\" ng-src=\"{{getImageURLByName(image.file)}}\"/>\n      </div>\n    </div>\n    <div class=\"cell\"><input type=\"text\" class=\"input form-control\" id=\"imageWidth\" title=\"Width\" ng-model=\"image.frameWidth\"></div>\n    <div class=\"cell\"><input type=\"text\" class=\"input form-control\" id=\"imageHeight\" title=\"Height\" ng-model=\"image.frameHeight\"></div>\n    <!--div class=\"cell\"><toggle-switch title=\"Animated\" ng-model=\"image.isSprite\"></toggle-switch></div>\n    <div class=\"cell frames\"><input type=\"text\" class=\"input form-control\" id=\"imageFrames\" title=\"Number of Frames\" ng-model=\"image.framesNumber\" ng-disabled=\"!image.isSprite\"></div-->\n    <div class=\"cell file\">{{image.file.substring(image.file.lastIndexOf(\'/\')+1)}}</div>\n    <div class=\"cell file-upload\">\n      <label class=\"btn btn-default\" title=\"Upload\" for=\"uploadFile_{{$index}}\">\n        <input id=\"uploadFile_{{$index}}\" type=\"file\" style=\"display:none;\" on-file-change=\"uploadImageFile\" ng-model=\"image.file\"><span class=\"glyphicon glyphicon-cloud-upload\" aria-hidden=\"true\"></span>\n        Upload\n      </label>\n    </div>\n    <div class=\"cell\">{{image.type || \'image\'}}</div>\n    <div class=\"cell delete-image\"><button class=\"btn btn-default\" title=\"Remove Image\" ng-click=\"deleteImage($index)\"><span class=\"glyphicon glyphicon-remove-sign\" aria-hidden=\"true\"></span> Remove</button></div>\n\n  </div>\n\n\n  <div class=\"error\" ng-if=\"validationError\">\n    <span class=\"glyphicon glyphicon-exclamation-