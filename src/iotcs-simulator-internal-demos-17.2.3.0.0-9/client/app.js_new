(function() {
  'use strict';

  var mod = angular.module('simulator');
  mod.controller('DashboardCtrl', ["$scope", "$rootScope", "$interval", function($scope, $rootScope, $interval) {

    $scope.mpsLabels = [];
    $scope.mpsSeries = ['Messages Per Second'];
    $scope.mpsData = [[]];
    $scope.mpsOptions = {
      maintainAspectRatio: false,
      animation: false,
      scaleShowVerticalLines: false
    };

    var messageCount = $rootScope.messageCount;
    var chartUpdateTimer = $interval(function() {
      var globalCount = $rootScope.messageCount;
      var count = globalCount - messageCount;
      messageCount = globalCount;
      $scope.mpsLabels.push('');
      $scope.mpsData[0].push(count);
      if ($scope.mpsLabels.length > 20) {
        $scope.mpsLabels.shift();
        $scope.mpsData[0].shift();
      }
    }, 1000);

    $scope.$on('$destroy', function() {
      $interval.cancel(chartUpdateTimer);
      chartUpdateTimer = undefined;
    });

    $rootScope.onAlert = function(messages) {
      messages.forEach(function(message) {
        var simpleDataRow = {
          id: message.id,
          source: message.source,
          type: message.type,
          priority: message.priority,
          eventTime: new Date(message.eventTime).toString(),
          format: message.payload.format,
          data: JSON.stringify(message.payload.data)
        };
        $rootScope.alertsData.unshift(simpleDataRow);
        if ($rootScope.alertsData.length > 100) {
          $rootScope.alertsData.pop();
        }
      });
    };

    $rootScope.pushData = function(container, data) {
      container.unshift(data);
      if (container.length > 100) {
        container.pop();
      }
    };

    $rootScope.onData = function(messages) {
      messages.forEach(function(message) {
        $rootScope.messageCount++;
        var simpleDataRow = {
          id: message.id,
          source: message.source,
          type: message.type,
          priority: message.priority,
          eventTime: new Date(message.eventTime).toString(),
          format: message.payload.format,
          data: JSON.stringify(message.payload.data)
        };
        if (simpleDataRow.format.indexOf('urn:oracle:iot:exploration:') !== 0) {
          $rootScope.pushData($rootScope.messagesData, simpleDataRow);
        } else {
          $rootScope.pushData($rootScope.analyticsData, simpleDataRow);
        }
      });
    };

    $scope.update = function() {
      $scope.appFound = $scope.$parent.isInitialized();
      if (!$scope.appFound) {
        // IoT application not found or still waiting for response
        return;
      }
      if ($rootScope.messageEnumerator) {
        return;
      }
      $rootScope.messageEnumerator = new iotcs.enterprise.MessageEnumerator($rootScope.simulatorGlobals.ea);
      $rootScope.messageEnumerator.setListener(null, null, $rootScope.onData);
      $rootScope.messageEnumerator.setListener(null, 'ALERT', $rootScope.onAlert);
    };

    $rootScope.$watch('simulatorGlobals.ea', function() {
      $scope.update();
    });

  }]);
}());

(function() {
  'use strict';

  var mod = angular.module('simulator');

  mod.controller('AssetsCtrl', ["$scope", "$rootScope", "$http", "growl", function($scope, $rootScope, $http, growl) {

    $scope.deviceMarkers = {};

    /**
     * Select a new map and perform needed initializations.
     */
    $scope.selectMap = function(map) {
      $scope.selectedMap = map;
      map.areas = [];

      $http.get('/iot/places/' + map.name + '/floorplan').then(function(res) {
        map.image = res.data.image.links[0].src;
        map.referencePoints = res.data.referencePoints;
      });

      var image = document.getElementById('mapImage');
      if (!image) {
        return;
      }
      image.onload = function() {
        // Resize canvas to match image size
        var canvas = document.getElementById('imageCanvas');
        // Check if canvas and map are there in case we navigated away from the page
        if (!canvas || !$scope.selectedMap) {
          return;
        }
        canvas.width = image.width;
        canvas.height = image.height;
        clearDropArea();

        map.geoFences.forEach(function(fence) {
          $http.get('/iot/fences/' + fence).then(function(res) {
            // jscs:disable requireCamelCaseOrUpperCaseIdentifiers
            var altRange = res.data.geoJSON.properties.altitude_range; // jshint ignore:line
            // jscs:enable requireCamelCaseOrUpperCaseIdentifiers
            var alt = altRange ? (altRange[1] + altRange[0]) / 2 : 0;
            // Create a separate area for each polygon
            res.data.geoJSON.geometry.polygons.forEach(function(polygon) {
              // Can there be multiple sets of coordinates in a poly?
              var fenceCoords = polygon.coordinates[0];
              var coordArray = [];
              var area = {
                id: res.data.name,
                shape: 'poly',
                alt: alt
              };
              for (var i = 0; i < fenceCoords.length; i++) {
                var xy = gpsToScreen(fenceCoords[i]);
                coordArray.push(xy[0]);
                coordArray.push(xy[1]);
              }
              area.coords = coordArray.join(',');
              map.areas.push(area);
            });
          });
        });
        placeMarkers();
      };
    };

    function toRadians(angle) {
      return angle * Math.PI / 180;
    }

    function toDegrees(angle) {
      return angle * 180 / Math.PI;
    }

    /**
     * Convert GPS coordinates to map coordinates using Web Mercator projection.
     * Since the floorplan is overlayed over the projection, we need to do this
     * conversion to preserve angles in geofences.
     */
    function gpsToMap(lonLat) {
      return [toRadians(lonLat[0]), Math.log(Math.tan(Math.PI / 4 + toRadians(lonLat[1]) / 2))];
    }

    /**
     * Convert map coords in Web Mercator projection to GPS coords
     */
    function mapToGps(xy) {
      return [toDegrees(xy[0]), toDegrees(2 * (Math.atan(Math.exp(xy[1])) - Math.PI / 4))];
    }

    /**
     * Convert GPS coordinates to coordinates relative to the currently selected floorplan image
     */
    function gpsToScreen(lonLat) {
      var mapImage = document.getElementById('mapImage');
      var scaleFactorX = mapImage.naturalWidth / mapImage.width;
      var scaleFactorY = mapImage.naturalHeight / mapImage.height;

      var imageP1 = $scope.selectedMap.referencePoints.imageP1.split(',');
      var imageP2 = $scope.selectedMap.referencePoints.imageP2.split(',');
      var mapP1 = $scope.selectedMap.referencePoints.mapP1.split(',');
      var mapP2 = $scope.selectedMap.referencePoints.mapP2.split(',');

      // Reference point coordinates are [y,x] and image y coordinate is counted from the bottom
      imageP1 = [+imageP1[1], mapImage.naturalHeight - imageP1[0]];
      imageP2 = [+imageP2[1], mapImage.naturalHeight - imageP2[0]];
      mapP1 = gpsToMap([mapP1[1], mapP1[0]]);
      mapP2 = gpsToMap([mapP2[1], mapP2[0]]);

      var Mtgt = transformCoords(mapP1, mapP2, imageP1, imageP2, gpsToMap(lonLat));

      Mtgt[0] /= scaleFactorX;
      Mtgt[1] /= scaleFactorY;
      return Mtgt;
    }

    /**
     * Convert coordinates relative to the currently selected floorplan image to GPS coordinates
     */
    function screenToGps(xy) {
      var mapImage = document.getElementById('mapImage');
      var scaleFactorX = mapImage.naturalWidth / mapImage.width;
      var scaleFactorY = mapImage.naturalHeight / mapImage.height;

      var imageP1 = $scope.selectedMap.referencePoints.imageP1.split(',');
      var imageP2 = $scope.selectedMap.referencePoints.imageP2.split(',');
      var mapP1 = $scope.selectedMap.referencePoints.mapP1.split(',');
      var mapP2 = $scope.selectedMap.referencePoints.mapP2.split(',');

      // Reference point coordinates are [y,x] and image y coordinate is counted from the bottom
      imageP1 = [+imageP1[1], mapImage.naturalHeight - imageP1[0]];
      imageP2 = [+imageP2[1], mapImage.naturalHeight - imageP2[0]];
      mapP1 = gpsToMap([mapP1[1], mapP1[0]]);
      mapP2 = gpsToMap([mapP2[1], mapP2[0]]);

      var mapCoords = transformCoords(imageP1, imageP2, mapP1, mapP2, [xy[0] * scaleFactorX, xy[1] * scaleFactorY]);
      return mapToGps(mapCoords);
    }

    /**
     * Transform the coordinates of point M given coordinates of two reference points
     * P1 and P2 in both systems (src and tgt).
     * Each point is represented as a two-element array ([x,y]).
     * Returns the coordinates of M in target system (Mtgt).
     */
    function transformCoords(P1src, P2src, P1tgt, P2tgt, Msrc) {
      // Distance from P1 to P2
      var Dsrc = Math.sqrt((P2src[0] - P1src[0]) * (P2src[0] - P1src[0]) + (P2src[1] - P1src[1]) *
        (P2src[1] - P1src[1]));
      var Dtgt = Math.sqrt((P2tgt[0] - P1tgt[0]) * (P2tgt[0] - P1tgt[0]) + (P2tgt[1] - P1tgt[1]) *
        (P2tgt[1] - P1tgt[1]));

      // Distance from P1 to M
      var Rsrc = Math.sqrt((Msrc[0] - P1src[0]) * (Msrc[0] - P1src[0]) + (Msrc[1] - P1src[1]) * (Msrc[1] - P1src[1]));
      var Rtgt = Rsrc * Dtgt / Dsrc;

      // Angle between (P1,P2) and (P1,M)
      // For simplification assuming the angle is the same in both systems
      // provided distances are small and we are away from the poles
      var theta = Math.atan2(Msrc[1] - P1src[1], Msrc[0] - P1src[0]) - Math.atan2(P2src[1] - P1src[1],
          P2src[0] - P1src[0]);

      // Angle between (1,0) and (P1,M) in tgt system
      var alpha = Math.atan2(P2tgt[1] - P1tgt[1], P2tgt[0] - P1tgt[0]) - theta;

      // Mtgt
      return [P1tgt[0] + Rtgt * Math.cos(alpha), P1tgt[1] + Rtgt * Math.sin(alpha)];

    }

    /**
     * Select a device and load the simulator model.
     */
    $scope.selectDevice = function(dev) {
      if ($scope.device === dev) {
        // Toggle selection if already selected
        delete $scope.device;
        return;
      }
      $scope.device = dev;
      if ($scope.device && !$scope.device.closed) {
        $http.get($scope.device.modelURL).then(function(res) {
          $scope.device.dm = res.data;
        });
      }
    };

    /**
     * Drag event visualization.
     */
    function handleDrag(target) {
      var coords = target.coords;

      if (coords) {
        var canvas = document.getElementById('imageCanvas');
        var context = canvas.getContext('2d');
        coords = coords.split(',');
        context.fillStyle = 'rgba(128, 128, 256, 0.4)';
        context.strokeStyle = '#4040ff';
        context.lineWidth = 3;
        context.font = '40px Arial';
        context.clearRect(0, 0, canvas.width, canvas.height);
        var textX;
        var textY;
        switch (target.shape) {
          case 'rect':
            context.strokeRect(coords[0], coords[1], coords[2] - coords[0], coords[3] - coords[1]);
            context.fillRect(coords[0], coords[1], coords[2] - coords[0], coords[3] - coords[1]);
            textX = coords[0];
            textY = coords[3];
            break;
          case 'poly':
            context.beginPath();
            context.moveTo(coords[0], coords[1]);
            textX = coords[0];
            textY = coords[1];
            for (var i = 2; i < coords.length - 1; i += 2) {
              context.lineTo(coords[i], coords[i + 1]);
              textX = Math.min(coords[i], textX);
              textY = Math.max(coords[i + 1], textY);
            }
            context.closePath();
            context.stroke();
            context.fill();
            break;
          case 'circle':
            context.beginPath();
            context.arc(coords[0], coords[1], coords[2], 0, 2 * Math.PI);
            context.closePath();
            context.stroke();
            context.fill();
            textX = coords[0] - coords[2];
            textY = coords[0] - coords[1];
        }
        context.fillStyle = 'white';
        context.fillText(target.id, textX, textY - 10);
      } else {
        clearDropArea();
      }
    }

    /**
     * Drag over handler for the image element.
     *
     * This handler is needed on IE which does not devliver drag events to 'area' elements.
     */
    $scope.dragOverImage = function(ev) {
      // Find the area element which is the actual target of the event
      var target = document.elementFromPoint(ev.clientX, ev.clientY);
      if (!target.id || /*target.id==='mapImage' ||*/ !target.getAttribute('ondrop') || target.getAttribute('draggable')) {
        return false;
      }
      ev.preventDefault();
      handleDrag(target);
    };

    /**
     * Drag over handler
     */
    $scope.dragOver = function(ev) {
      if (!ev.target.id || ev.target.id === 'mapImage' || !ev.target.getAttribute('ondrop') ||
        ev.target.getAttribute('draggable')) {
        return false;
      }
      ev.preventDefault();
      handleDrag(ev.target);
    };

    /**
     * Drag start handler
     */
    $scope.drag = function(ev) {
      ev.dataTransfer.setData('text', ev.target.id);
    };

    /**
     * Place marker on the map
     */
    function placeMarker(element, mapMarker) {
      // Transform map cordinates to absolute coordinates within assetsView
      var assetsView = document.getElementById('assetsView');
      var parentBounds = assetsView.getBoundingClientRect();
      var mapImage = document.getElementById('mapImage');
      var mapBounds = mapImage.getBoundingClientRect();

      // Transform GPS coords to screen
      var xy = gpsToScreen([mapMarker.lon, mapMarker.lat]);
      var markerX = mapBounds.left - parentBounds.left + xy[0];
      var markerY = mapBounds.top - parentBounds.top + xy[1];

      element.style.top = markerY + 'px';
      element.style.left = markerX + 'px';
      element.style.position = 'absolute';
      element.className = 'draggable-map';
    }

    function moveDeviceToMap(device, mapMarker) {
      var element = document.getElementById(device.id);
      $scope.deviceMarkers[device.id] = mapMarker;
      placeMarker(element, mapMarker);
      updateDeviceLocation(device, mapMarker);
    }

    /**
     * Place device markers on the currently selected map
     */
    function placeMarkers() {
      $rootScope.devices.forEach(function(device) {
        var mapMarker = $scope.deviceMarkers[device.id];
        var element = document.getElementById(device.id);
        if (element) {
          if (mapMarker && mapMarker.map === $scope.selectedMap.name) {
            // The device belongs to the current map - place the marker
            placeMarker(element, mapMarker);
            updateDeviceLocation(device, mapMarker);
          } else {
            // The device not on the current map - clear marker
            resetDeviceMarker(device);
          }
        }
      });
    }

    function updateDeviceLocation(device, location) {
      var patch = {};
      Object.keys(device).forEach(function(key) {
        if (key.indexOf('.ora_latitude') !== -1) {
          patch[key] = location.lat;
        } else if (key.indexOf('.ora_longitude') !== -1) {
          patch[key] = location.lon;
        } else if (key.indexOf('.ora_altitude') !== -1) {
          patch[key] = location.alt;
        }
      });
      $http.post('/sim/devices/' + device.id, patch);
    }

    /**
     * Drop event handler.
     * Moves the device to/from map.
     */
    $scope.drop = function(ev) {
      ev.preventDefault();

      var data = ev.dataTransfer.getData('text');
      var target = document.getElementById(ev.target.id);
      var mapImage = document.getElementById('mapImage');
      var mapBounds = mapImage.getBoundingClientRect();

      $rootScope.devices.forEach(function(device) {
        if (device.id === data) {
          if (target.id === 'left-panel') {
            clearDropArea();
            resetDeviceMarker(device);
            delete $scope.deviceMarkers[device.id];
            return;
          }
          // Transform coordinates relative to the map image to lat and lon
          var lonLat = screenToGps([ev.clientX - mapBounds.left, ev.clientY - mapBounds.top]);
          // There is no altitude associated with the floorplan so choose an alt for one of the areas.
          var alt = $scope.selectedMap.areas[0].alt;

          var mapMarker = {lon: lonLat[0], lat: lonLat[1], alt: alt};
          moveDeviceToMap(device, mapMarker);

          console.log(data + ' moved to Lat:' + mapMarker.lat + ' Lon:' + mapMarker.lon + ' Alt:' + alt);
          mapMarker.map = $scope.selectedMap.name;
        }
      });
    };

    function resetDeviceMarker(device) {
      var element = document.getElementById(device.id);
      element.style.top = '0';
      element.style.left = '0';
      element.style.position = 'relative';

      element.className = 'device-btn draggable';
    }

    function clearDropArea() {
      var canvas = document.getElementById('imageCanvas');
      var context = canvas.getContext('2d');
      context.clearRect(0, 0, canvas.width, canvas.height);
    }

    /**
     * Map reset.
     * Removes all devices from map.
     */
    $scope.reset = function() {
      clearDropArea();
      $rootScope.devices.forEach(resetDeviceMarker);
      delete $scope.device;
    };

    $scope.save = function() {
      $http.post('/sim/maps/maps.json', {markers: $scope.deviceMarkers}).then(function() {
        growl.success('Map saved');
      }, function() {
        growl.error('Map could not be saved');
      });
    };

    $scope.getMapForDevice = function(dev) {
      var marker = $scope.deviceMarkers[dev.id];
      return marker ? marker.map : undefined;
    };

    $scope.getBarValue = function(attribute) {
      var value = $scope.device[attribute.name];
      if (attribute.type === 'BOOLEAN') {
        return value ? 100 : 0;
      }
      if (attribute.range) {
        var range = attribute.range.split(',');
        return (value - range[0]) * 100.0 / (range[1] - range[0]);
      }
      return 0;
    };
    $scope.getMonitorBarValue = function(monitor) {
      if (monitor.type === 'BOOLEAN') {
        return monitor.value ? 100 : 0;
      }
      if (monitor.range) {
        var range = monitor.range.split(',');
        return (monitor.value - range[0]) * 100.0 / (range[1] - range[0]);
      }
      return 0;
    };

    $scope.getDisplayFormat = function(attribute) {
      if (attribute.format) {
        if (attribute.type === 'NUMBER') {
          return attribute.format.length - attribute.format.lastIndexOf('.') - 1;
        } else if (attribute.type === 'DATETIME') {
          return attribute.format;
        }
      }
      return 0;
    };

    if (!$scope.maps) {
      $http.get('/sim/maps/maps.json').then(function(res) {
        //$scope.maps = res.data.maps;
        $scope.deviceMarkers = res.data.markers;
        //$scope.selectMap($scope.maps[0]);
        //angular.element(document).ready(placeMarkers);
      });
      $http.get('/iot/places').then(function(res) {
        $scope.maps = res.data.items;
        $scope.selectMap($scope.maps[0]);
      });
    } else {
      $scope.selectMap($scope.maps[0]);
      angular.element(document).ready(placeMarkers);
    }
  }]);
}());

