  var mod = angular.module('simulator');

  mod.controller('GatewayCtrl', ["$scope", "$state", "$interval", "$filter", "$http", "$log", "$stateParams", "$rootScope", "websocket", function($scope, $state, $interval, $filter, $http, $log, $stateParams,
                                         $rootScope, websocket) {

    $scope.name = 'GatewayCtrl';
    // For gateway we still should have "Device Simulators" tab active
    $rootScope.appTabs.find(function(tab) {
      return tab.route === 'sim.device';
    }).active = true;
    $scope.device = $rootScope.devices.find(function(dev) {
      return dev.id === $stateParams.id;
    });

    if (!$scope.device) {
      // Do nothing if no gateway created yet
      return;
    }
    // Check if we are in the correct controller for the Gateway and switch if needed
    if ($scope.device.deviceName !== 'Gateway') {
      $state.go('sim.device', {'id': $stateParams.id}, {reload: true});
