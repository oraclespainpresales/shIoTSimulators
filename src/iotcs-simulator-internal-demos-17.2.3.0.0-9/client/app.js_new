    $rootScope.activeDeviceId = $scope.device.id;

    var initLocation = function() {
      if ($scope.device.location) {
        $scope.coordinates = Object.keys($scope.device.location);
      }
    };

    initLocation();

    var onGatewayInitialization = function(device) {
      if (device.id === $scope.device.id) {
        $scope.device.location = device.location;
        initLocation();
        $scope.device.icds = device.icds;
      }
    };

    websocket.on('onGatewayInitialization', onGatewayInitialization);

  }]);

