      //!(selectedType && (gatewayId && !isIndirect) ((deviceId && (secret || isIndirect)) || registerNewDevice))
      if (!$scope.selectedType) {
        return false;
      }
      if ($scope.isIndirect && !$scope.selectedGatewayId) {
        return false;
      }
      if (!$scope.registerNewDevice) {
        if (!($scope.deviceId && ($scope.secret || $scope.isIndirect))) {
          return false;
        }
      }
      return true;
    };

    $scope.ok = function() {
      // $log.info('ok() called at: ' + new Date());
      $modalInstance.close();
      // $log.info('emit(createDevice) called at: ' + new Date());
      $rootScope.$emit('createDevice', $scope.deviceId, $scope.secret, $rootScope.deviceNamesToIds[$scope.selectedType],
        $scope.registerNewDevice, $scope.name, $scope.isIndirect, $scope.selectedGatewayId,
        {
          description: $scope.description,
          manufacturer: $scope.manufacturer,
          serialNumber: $scope.serialNumber,
          modelNumber: $scope.modelNumber
        },
        {
          latitude: $scope.latitude,
          longitude: $scope.longitude,
          altitude: $scope.altitude
        });
    };
    $scope.cancel = function() {
      $modalInstance.dismiss('cancel');
    };
    $scope.selectDevice = function(type) {
      $scope.selectedType = type;
    };
    $scope.selectGateway = function(gatewayId) {
      $scope.selectedGatewayId = gatewayId;
    };
    // Generates unique identifier
    $scope.generateId = function() {
      $scope.deviceId = uuidv4();
    };
  }]);

}());

