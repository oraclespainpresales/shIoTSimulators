(function() {
  'use strict';

  var mod = angular.module('simulator');

  /* Custom directive to handle file input binding */
  mod.directive('onFileChange', function() {
    return {
      require: 'ngModel',
      restrict: 'A',
      link: function(scope, element, attrs, ngModel) {
        var onFileChange = scope.$eval(attrs.onFileChange);
        element.bind('change', function() {
          onFileChange(element[0], ngModel);
        });
      }
    };
  });

  mod.controller('EditorCtrl', ["$scope", "$rootScope", "$http", "growl", function($scope, $rootScope, $http, growl) {

    $scope.deviceModels = {};
    var errorItems = [];
    var originalName = ''; // Name under which the simulator was created or opened to keep track of name changes

    var backupSimulatorModel;

    function setSimulatorModel(model) {
      $scope.simulatorModel = model;
      backupSimulatorModel = JSON.parse(JSON.stringify(model));
    }

    /**
     * Return device ID for the currently selected device
     */
    function getDeviceId() {
      var deviceId = $rootScope.deviceNamesToIds[$scope.simulatorName];
      if (deviceId) {
        return deviceId;
      } else {
        return $scope.simulatorModel.deviceName.toLowerCase().replace(/ /g, '_');
      }
    }

    /**
     * Create new empty simulator
     */
    $scope.createSimulator = function() {
      if (!checkForUnsavedChanges()) {
        return;
      }
      $scope.validationError = '';
      errorItems = [];
      var name = '';
      var model = {
        deviceName: name,
        messagingInterval: $rootScope.config.messagingInterval || 1000,
        messagesLimit: 0,
        images: [
          {
            'name': 'isOnImage',
            'file': '$shared/assets/images/sensorOn.png',
            'frameWidth': 700,
            'frameHeight': 500
          },
          {
            'name': 'isOffImage',
            'file': '$shared/assets/images/sensorOff.png',
            'frameWidth': 700,
            'frameHeight': 500
          }
        ],
        deviceModelsExtensions: [{
          'urn': null,
          'attributes': [],
          'formats': [],
          'actions': []
        }],
        events: [],
        isOnAttribute: {initialValue: true}
      };
      $scope.simulatorName = name;
      originalName = '';
      setSimulatorModel(model);
    };

    $scope.selectSimulator = function(name) {
      if (!checkForUnsavedChanges()) {
        return;
      }
      $scope.validationError = '';
      $scope.simulatorName = name;
      originalName = name;
      var deviceType = getDeviceId();
      var fileName = deviceType + '_simulator.json';

      $http.get('/' + deviceType + '/' + fileName).then(function(res) {
        setSimulatorModel(res.data);
      });

    };

    /**
     * Save simulator
     */
    $scope.save = function() {
      if (!validate()) {
        return;
      }

      if ($scope.simulatorName !== originalName && $rootScope.deviceNamesToIds[$scope.simulatorName]) {
        // Creating new simulator with a name which already exists
        if (!confirm('A simulator named ' + $scope.simulatorName + ' already exists.\nDo you want to replace it?')) {
          return;
        }
      }

      var deviceType = getDeviceId();

      $http.post('/sim/deviceTypes/' + deviceType, $scope.simulatorModel).then(function(res) {
        if (res.statusCode !== 500) {
          growl.success('Simulator saved');
          //Simulator successfully saved - add it to the list of available simulators
          $rootScope.deviceNamesToIds[$scope.simulatorName] = deviceType;
          backupSimulatorModel = JSON.parse(JSON.stringify($scope.simulatorModel));
          originalName = $scope.simulatorName;
        }
      });

    };

    /**
     * Reset simulator form
     */
    $scope.reset = function() {
      if (confirm('Do you want to reset the changes?\nAll edits since the last save will be lost.')) {
        $scope.simulatorModel = JSON.parse(JSON.stringify(backupSimulatorModel));
        $scope.simulatorName = $scope.simulatorModel.deviceName;
        $scope.validationError = '';
        errorItems = [];
      }
    };

    /**
     * Delete simulator files
     */
    $scope.delete = function() {
      var deviceType = $rootScope.deviceNamesToIds[$scope.simulatorName];

      if (!deviceType) {
        alert('Unknown simulator: ' + $scope.simulatorName);
      } else if (confirm('Do you want to permanently remove simulator ' + $scope.simulatorName + '?')) {
        $http.delete('/sim/deviceTypes/' + deviceType).then(function(res) {
          if (res.statusCode !== 500) {
            growl.success('Simulator deleted');
            delete $rootScope.deviceNamesToIds[$scope.simulatorName];
            $scope.createSimulator();
          }
        });
      }
    };

    function validateType(object, property, type) {
      var value = object[property];
      // Don't check variable expressions for now
      if ((typeof value === 'string') && value.indexOf('$') === 0) {
        return value;
      }
      if (type === 'BOOLEAN') {
        if (typeof value === 'boolean') {
          return value;
        }
        if ((typeof value === 'string') && value.toLowerCase() === 'true') {
          return true;
        }
        if ((typeof value === 'string') && value.toLowerCase() === 'false') {
          return false;
        }
        $scope.validationError += 'Incorrect boolean value ' + value + '\n';
        addError(object, property);
      } else if (type === 'INTEGER') {
        var intValue = parseInt(value, 10);
        if (!isNaN(intValue)) {
          return intValue;
        }
        $scope.validationError += 'Incorrect integer value ' + value + '\n';
        addError(object, property);
      } else if (type === 'NUMBER') {
        var numValue = parseFloat(value);
        if (!isNaN(numValue)) {
          return numValue;
        }
        $scope.validationError += 'Incorrect numeric value ' + value + '\n';
        addError(object, property);
      } else if (type === 'DATETIME') {
        var dateValue = Date.parse(value);
        if (!isNaN(dateValue)) {
          return new Date(value).toJSON();
        }
        $scope.validationError += 'Incorrect date value ' + value + '\n';
        addError(object, property);
      } else if (type === 'AUTO') {
        // Autodetect type
        if (typeof value === 'string') {
          if (value.toLowerCase() === 'true') {
            return true;
          }
          if (value.toLowerCase() === 'false') {
            return false;
          }
          if (!isNaN(value)) {
            return parseFloat(value);
          }
          if (!isNaN(Date.parse(value))) {
            return new Date(value).toJSON();
          }
        }
      }
      return value;
    }

    /**
     * Validate form before saving
     */
    function validate() {
      $scope.validationError = '';
      errorItems = [];

      // Device name
      if (!$scope.simulatorModel.deviceName || $scope.simulatorModel.deviceName === '') {
        $scope.validationError += 'Simulator name must be specified\n';
        addError($scope.simulatorModel, 'deviceName');
      }
      // Device models non-empty
      if ($scope.simulatorModel.deviceModelsExtensions.length === 0) {
        $scope.validationError += 'At least one device model is required\n';
        addError($scope.simulatorModel, 'deviceModelsExtensions');
      }

      $scope.simulatorModel.deviceModelsExtensions.forEach(function(dm, index) {
        // Device model
        var deviceModel = $scope.deviceModels[dm.urn];
        if (!dm.urn) {
          $scope.validationError += 'Undefined model for Device #' + (index + 1) + '\n';
          addError(dm, 'urn');
        } else if (!deviceModel) {
          // Can't validate this device model
          $scope.validationError += 'Model ' + dm.urn + ' does not exist on the server.\n';
          return;
        }
        dm.attributes.forEach(function(attr) {
          // Initial value
          deviceModel.attributes.forEach(function(dmAttr) {
            if (dmAttr.name === attr.name) {
              // We allow undefined initial value so only check it if defined
              if (attr.initialValue) {
                attr.initialValue = validateType(attr, 'initialValue', dmAttr.type);
              }
            }
          });
          // Function arguments
          if (attr.onFunction) {
            $scope.getArguments($scope.functions, attr.onFunction).forEach(function(arg) {
              if (!attr.onArguments || attr.onArguments[arg.name] === undefined) {
                $scope.validationError += 'Missing argument for function ' + attr.onFunction + '\n';
                addError(attr, 'onFunction');
              } else {
                attr.onArguments[arg.name] = validateType(attr.onArguments, arg.name, arg.type);
              }
            });
          }
          if (attr.offFunction) {
            $scope.getArguments($scope.functions, attr.offFunction).forEach(function(arg) {
              if (!attr.offArguments || attr.offArguments[arg.name] === undefined) {
                $scope.validationError += 'Missing argument for function ' + attr.offFunction + '\n';
                addError(attr, 'offFunction');
              } else {
                attr.offArguments[arg.name] = validateType(attr.offArguments, arg.name, arg.type);
              }
            });
          }
        });

        // Alerts
        if (dm.formats) {
          dm.formats.forEach(function(alert) {
            if (alert.image && !alert.imageTime) {
              alert.imageTime = 2000;
            } else if (alert.image) {
              if (isNaN(alert.imageTime)) {
                $scope.validationError += 'Incorrect non-integer value for alert image time "' +
                  alert.imageTime + '"\n';
                addError(alert, 'imageTime');
              } else {
                alert.imageTime = Math.round(alert.imageTime);
              }
            } else {
              delete alert.imageTime;
            }
            deviceModel.formats.forEach(function(dmFormat) {
              if (dmFormat.urn === alert.urn) {
                if (!alert.visible) {
                  // Alert condition arguments
                  if (alert.condition) {
                    $scope.getArguments($scope.conditions, alert.condition).forEach(function(arg) {
                      if (!alert.conditionArguments || alert.conditionArguments[arg.name] === undefined) {
                        $scope.validationError += 'Missing argument for alert condition ' + alert.condition + '\n';
                        addError(alert, 'condition');
                      } else {
                        alert.conditionArguments[arg.name] = validateType(alert.conditionArguments, arg.name, arg.type);
                      }
                    });
                  }
                }
                if (alert.value && alert.value.fields) {
                  alert.value.fields.forEach(function(field) {
                    dmFormat.value.fields.forEach(function(dmField) {
                      if (dmField.name === field.name) {
                        field.value = validateType(field, 'value', dmField.type);
                      }
                    });
                  });
                }
              }
            });
          });
        }

        // Actions
        if (dm.actions) {
          dm.actions.forEach(function(action) {
            deviceModel.actions.forEach(function(dmAction) {
              if (dmAction.name === action.name) {
                // Action type
                action.value = validateType(action, 'value', dmAction.argType);
                // Action handler arguments
                if (action.actionHandler) {
                  $scope.getArguments($scope.handlers, action.actionHandler).forEach(function(arg) {
                    if (!action.actionArguments || action.actionArguments[arg.name] === undefined) {
                      $scope.validationError += 'Missing argument for action handler ' + action.actionHandler + '\n';
                      addError(action, 'actionHandler');
                    } else {
                      action.actionArguments[arg.name] = validateType(action.actionArguments, arg.name, arg.type);
                    }
                  });
                }
              }
            });
          });
        }
      });

      // Events
      if ($scope.simulatorModel.events) {
        $scope.simulatorModel.events.forEach(function(event) {
          if (!event.name || event.name === '') {
            $scope.validationError += 'Event name must be specified\n';
            addError(event, 'name');
          }
          if (event.attributes) {
            Object.keys(event.attributes).forEach(function(attr) {
              if (attr.indexOf('_new_attribute') === 0) {
                // Event attribute
                $scope.validationError += 'Undefined attribute for event ' + event.name + '\n';
                addError(attr, null);
              } else if (!event.attributes[attr].onEventFunction || event.attributes[attr].onEventFunction === '') {
                // Event attribute function
                $scope.validationError += 'Unspecified function for event ' + event.name + '\n';
                addError(event.attributes[attr], 'onEventFunction');
              } else {
                // Event attribute function arguments
                $scope.getArguments($scope.functions, event.attributes[attr].onEventFunction).forEach(function(arg) {
                  if (!event.attributes[attr].onEventArguments ||
                    event.attributes[attr].onEventArguments[arg.name] === undefined) {
                    $scope.validationError += 'Missing argument for function ' +
                      event.attributes[attr].onEventFunction + '\n';
                    addError(event.attributes[attr], 'onEventFunction');
                  } else {
                    event.attributes[attr].onEventArguments[arg.name] =
                      validateType(event.attributes[attr].onEventArguments, arg.name, arg.type);
                  }
                });
              }
            });
          }
        });
      }
      return $scope.validationError === '';
    }

    /**
     * Add an object property to the list of errors.
     */
    function addError(object, property) {
      errorItems.push({object: object, property: property});
    }

    /**
     * Check if an error was detected in an object property during validation.
     * Return appropriate css class for this item..
     */
    $scope.checkForErrors = function(object, property) {
      for (var i = 0; i < errorItems.length; i++) {
        if (errorItems[i].object === object && (!property || errorItems[i].property === property)) {
          return 'error-highlight';
        }
      }
      return '';
    };

    $scope.addAttribute = function(event) {
      // Generate temporary unique attribute name
      event.attributes = event.attributes || {};

      var i = 1;
      while (event.attributes['_new_attribute_' + i]) {
        i++;
      }
      event.attributes['_new_attribute_' + i] = {};
    };

    $scope.setAttributeName = function(event, tempName, name) {
      event.attributes[name] = event.attributes[tempName];
      // Clean-up temporary names
      delete event.attributes[name].selectedName;
      delete event.attributes[tempName];
    };

    $scope.deleteAttribute = function(event, name) {
      delete event.attributes[name];
    };

    $scope.addDeviceModel = function() {
      var deviceModel = {
        'urn': null,
        'attributes': [],
        'formats': [],
        'actions': []
      };
      $scope.simulatorModel.deviceModelsExtensions.push(deviceModel);
    };

    var defaultValues = {
      'NUMBER': 0,
      'INTEGER': 0,
      'BOOLEAN': false,
      'STRING': '',
      'DATETIME': new Date().toJSON()
    };

    function genInitialValue(attribute) {
      if (attribute.range) {
        var ranges = attribute.range.split(',');
        var middle = ((+ranges[1]) + (+ranges[0])) / 2;
        if (attribute.type === 'INTEGER') {
          middle = Math.round(middle);
        }
        return middle;
      } else {
        return defaultValues[attribute.type];
      }
    }

    $scope.populateDeviceModel = function(deviceModel) {
      var attrs = $scope.deviceModels[deviceModel.urn].attributes; //res.data.attributes;
      var formats = $scope.deviceModels[deviceModel.urn].formats; //res.data.formats;
      var actions = $scope.deviceModels[deviceModel.urn].actions; //res.data.actions;
      for (var i = 0; i < attrs.length; i++) {
        deviceModel.attributes[i] = {
          name: attrs[i].name,
          displayName: attrs[i].name,
          visible: attrs[i].type !== 'DATETIME',
          chartable: attrs[i].type === 'NUMBER' || attrs[i].type === 'INTEGER',
          format: (attrs[i].type === 'NUMBER') ? '###.#' : '####',
          initialValue: genInitialValue(attrs[i])
        };
      }
      // jshint shadow:true
      for (var i = 0; i < formats.length; i++) {
        deviceModel.formats[i] = {
          urn: formats[i].urn,
          visible: true,
          displayName: formats[i].urn.substring(formats[i].urn.lastIndexOf(':') + 1)
        };
        if (formats[i].value && formats[i].value.fields) {
          deviceModel.formats[i].value = {
            fields: []
          };
          for (var j = 0; j < formats[i].value.fields.length; j++) {
            deviceModel.formats[i].value.fields[j] = {
              name: formats[i].value.fields[j].name,
              value: defaultValues[formats[i].value.fields[j].type]
            };
          }
        }
      }
      for (var i = 0; i < actions.length; i++) {
        deviceModel.actions[i] = {
          name: actions[i].name,
          value: defaultValues[actions[i].argType]
        };
      }
    };

    $scope.deleteDeviceModel = function(index) {
      $scope.simulatorModel.deviceModelsExtensions.splice(index, 1);
    };

    $scope.addEvent = function() {
      var event = {
        'name': 'eventName',
        'description': 'Event Description',
        'exclusive': true,
        'attributes': {}
      };

      $scope.simulatorModel.events.push(event);
    };

    $scope.deleteEvent = function(eventIndex) {
      $scope.simulatorModel.events.splice(eventIndex, 1);
    };

    $scope.cloneEvent = function(eventIndex) {
      $scope.simulatorModel.events.splice(eventIndex, 0,
        JSON.parse(JSON.stringify($scope.simulatorModel.events[eventIndex])));
    };

    $scope.addImage = function() {
      var image = {
        'name': 'imageName',
        'file': '$shared/assets/images/sensorOff.png',
        'frameWidth': 700,
        'frameHeight': 500
      };

      $scope.simulatorModel.images.push(image);
    };

    $scope.deleteImage = function(imageIndex) {
      $scope.simulatorModel.images.splice(imageIndex, 1);
    };

    $scope.getArguments = function(functions, functionName) {
      for (var i = 0; i < functions.length; i++) {
        if (functionName === functions[i].name) {
          return functions[i].arguments;
        }
      }
      return [];
    };

    $scope.getImageURLByName = function(fileName) {
      if (fileName.indexOf('$shared') === 0) {
        return fileName.substring(7);
      }
      return getDeviceId() + '/' + fileName;
    };

    $scope.getImageURL = function(imageId) {
      for (var i = 0; i < $scope.simulatorModel.images.length; i++) {
        if (imageId === $scope.simulatorModel.images[i].name) {
          return $scope.getImageURLByName($scope.simulatorModel.images[i].file);
        }
      }
      return null;
    };

    function uploadFile(dataFile, dirName, callback) {
      var deviceType = getDeviceId();
      var fileName = 'devices/' + deviceType + '/' + dirName + '/' + dataFile.name;
      $http.post(fileName, dataFile, {
        headers: {'Content-Type': undefined},
        transformRequest: angular.identity
      }).then(function() {
        callback(fileName);
        growl.success('File uploaded');
      }, function() {
        growl.error('File could not be uploaded');
      });
    }

    $scope.uploadDataFile = function(inputElem, ngModel) {
      uploadFile(inputElem.files[0], 'lib', function(fileName) {
        ngModel.$setViewValue(fileName);
      });
    };

    $scope.uploadImageFile = function(inputElem, ngModel) {
      var imageFile = inputElem.files[0];
      uploadFile(imageFile, 'images', function() {
        var index = inputElem.id.substring(inputElem.id.indexOf('_') + 1);
        var extension = imageFile.name.substr(imageFile.name.lastIndexOf('.') + 1);
        ngModel.$setViewValue('images/' + imageFile.name);
        if (extension === 'css') {
          $scope.simulatorModel.images[index].type = 'css_animation';
        } else if (extension === 'mp4' || extension === 'webm') {
          $scope.simulatorModel.images[index].type = 'video/' + extension;
        } else {
          $scope.simulatorModel.images[index].type = 'image';
        }
      });
    };

    $scope.objectSize = function(o) {
      if (!o) {
        return 0;
      }
      return Object.keys(o).length;
    };

    /**
     * Check if there are unsaved changes and ask the user to confirm navigation
     */
    function checkForUnsavedChanges() {
      // Use angular.toJson which ignores $$ properties added by angular
      if (angular.toJson($scope.simulatorModel) !== angular.toJson(backupSimulatorModel)) {
        return confirm('Unsaved changes in ' + $scope.simulatorName +
          ' will be lost.\nPress OK to proceed or Cancel to continue editing.');
      }
      return true;
    }

    $scope.$on('$stateChangeStart', function(event) {
      if (!checkForUnsavedChanges()) {
        event.preventDefault();
      }
    });

    window.onbeforeunload = function() {
      if (angular.toJson($scope.simulatorModel) !== angular.toJson(backupSimulatorModel)) {
        return 'Unsaved changes in ' + $scope.simulatorName + ' will be lost.';
      }
    };

    $http.get('/sim/functions').then(function(res) {
      $scope.functions = res.data.functions;
      $scope.conditions = res.data.conditions;
