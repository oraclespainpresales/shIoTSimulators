(function() {
  'use strict';

  var mod = angular.module('simulator');

  Math.log10 = Math.log10 || function(x) {
      return Math.log(x) / Math.LN10;
    };

  // We use this function to avoid multiple invocations of the function fn on frequent UI events (e.g. when resizing window)
  var debounce = function(fn, delay) {
    var timer = null;
    return function() {
      var context = this;
      var args = arguments;
      clearTimeout(timer);
      timer = setTimeout(function() {
        fn.apply(context, args);
      }, delay);
    };
  };

  mod.controller('DeviceCtrl', ["$scope", "$window", "$state", "$interval", "$filter", "$http", "$log", "$stateParams", "$rootScope", "growl", function($scope, $window, $state, $interval, $filter, $http, $log, $stateParams,
                                        $rootScope, growl) {

    var reload = false;
    var animation;
    $scope.name = 'DeviceCtrl';
    // Enforcing "Device Simulators" tab is active
    $rootScope.appTabs.find(function(tab) {
      return tab.route === 'sim.device';
    }).active = true;

    if ($stateParams.id && ($stateParams.id !== '0')) {
      for (var i = 0; i < $rootScope.devices.length; i++) {
        if ($rootScope.devices[i].id === $stateParams.id) {
          $scope.device = $rootScope.devices[i];
        }
      }
    }

    if (!$stateParams.id || (String($stateParams.id) === '0') || !$scope.device) {
      reload = true;
      if ($rootScope.activeDeviceId !== '0') {
        $stateParams.id = $rootScope.activeDeviceId;
        /* jshint shadow:true */
        for (var i = 0; i < $rootScope.devices.length; i++) {
          if ($rootScope.devices[i].id === $rootScope.activeDeviceId) {
            $scope.device = $rootScope.devices[i];
          }
        }

      } else if ($rootScope.devices[0]) {
        $scope.device = $rootScope.devices[0];
      } else {
        // Do nothing if no devices created yet
        return;
      }
    }

    $rootScope.activeDeviceId = $scope.device.id;

    // Check if we are in the correct controller for the device and switch if needed
    if ($scope.device.deviceName === 'Gateway') {
      $state.go('sim.gateway', {'id': $scope.device.id}, {reload: true});
    } else if (reload) {
      $state.go('sim.device', {'id': $scope.device.id}, {reload: true});
    }

    $scope.deviceTabActive = $rootScope.tabsState[$scope.device.id].deviceTabActive;

    $scope.setDeviceTabActive = function(deviceTabActive) {
      $scope.deviceTabActive = deviceTabActive;
      $rootScope.tabsState[$scope.device.id].deviceTabActive = deviceTabActive;
    };

    $scope.window = $window;
    $scope.getImageScaleFactor = function() {
      if (!$scope.device.stateImage) {
        return 1.0;
      }
      var maxWidth = $window.innerWidth - 620; // this number approximately corresponds to the width of left and right panels
      if ($window.innerWidth < 600) {
        maxWidth = $window.innerWidth * 0.48;
      } else if ($window.innerWidth < 1000) {
        maxWidth = $window.innerWidth - 420;
      }
      if ($scope.device.stateImage.frameWidth < maxWidth) {
        return 1;
      } else {
        return maxWidth * 1.0 / $scope.device.stateImage.frameWidth;
      }
    };

    $scope.imageScaleFactor = $scope.getImageScaleFactor();

    $scope.getImageBackgroundWidth = function() {
      if (!$scope.device.stateImage) {
        return 0; // this is to avoid console errors while image isn't loaded yet
      }
      return Math.floor($scope.device.stateImage.frameWidth * $scope.imageScaleFactor);// $scope.getImageScaleFactor());
    };

    $scope.getImageBackgroundHeight = function() {
      if (!$scope.device.stateImage) {
        return 0; // this is to avoid console errors while image isn't loaded yet
      }
      return Math.floor($scope.device.stateImage.frameHeight * $scope.imageScaleFactor);// $scope.getImageScaleFactor());
    };

    $scope.updateImageSize = function() {
      $scope.imageScaleFactor = $scope.getImageScaleFactor();
      $scope.imageBackgroundWidth = $scope.getImageBackgroundWidth();
      $scope.imageBackgroundHeight = $scope.getImageBackgroundHeight();
    };

    $scope.updateImageSize();

    $(window).on('resize.doResize', debounce(function() {
      $scope.updateImageSize();
    }, 250));

    // Cache custom controls and monitors, but allow value binding
    if ($scope.device.controls) {
      $scope.controls = $scope.device.controls;
    }
    if ($scope.device.monitors) {
      $scope.monitors = $scope.device.monitors;
    }

    $http.get($scope.device.modelURL).then(function(res) {
      $scope.device.dm = res.data;
    });

    $scope.getBarValue = function(attribute) {
      var value = $scope.device[attribute.name];
      if (attribute.type === 'BOOLEAN') {
        return value ? 100 : 0;
      }
      if (attribute.range) {
        var range = attribute.range.split(',');
        return (value - range[0]) * 100.0 / (range[1] - range[0]);
      }
      return 0;
    };
    $scope.getMonitorBarValue = function(monitor) {
      if (monitor.type === 'BOOLEAN') {
        return monitor.value ? 100 : 0;
      }
      if (monitor.range) {
        var range = monitor.range.split(',');
        return (monitor.value - range[0]) * 100.0 / (range[1] - range[0]);
      }
      return 0;
    };

    $scope.getDisplayFormat = function(attribute) {
      if (attribute.format) {
        if (attribute.type === 'NUMBER') {
          return attribute.format.length - attribute.format.lastIndexOf('.') - 1;
        } else if (attribute.type === 'DATETIME') {
          return attribute.format;
        }
      }
      return 0;
    };

    $scope.onControlChange = function(controlName, controlValue) {
      var payload = {};
      payload[controlName] = controlValue;
      // console.log("Sending", controlName, "=", controlValue);
      $http.post('/sim/devices/' + $scope.device.id + '/control', payload);
    };

    $scope.updateAttribute = function(name, value) {
      var payload = {};
      payload[name] = value;
      $scope.device[name] = value;
      $http.post('/sim/devices/' + $scope.device.id, payload);
    };

    $scope.switchEvent = function(eventName) {
      var payload = {};
      var event;
      for (var i = 0; i < $scope.device.dm.events.length; i++) {
        if ($scope.device.dm.events[i].name === eventName) {
          event = $scope.device.dm.events[i];
        }
      }
      console.log('Event: ' + eventName + ' ' + $scope.device[eventName]);
      if ($scope.device[eventName]) {
        growl.error('Event: ' + event.description);
      }
      payload[eventName] = $scope.device[eventName];
      $http.post('/sim/devices/' + $scope.device.id + '/event', payload);
    };

    $scope.switchOnOff = function() {
      var payload = {
        value: $scope.device.isOn
      };
      $http.post('/sim/devices/' + $scope.device.id + '/on', payload);
    };

    $scope.sendAlert = function(alertName) {
      var alert;
      for (var i = 0; i < $scope.device.dm.visibleAlerts.length; i++) {
        if ($scope.device.dm.visibleAlerts[i].name === alertName) {
          alert = $scope.device.dm.visibleAlerts[i];
        }
      }

      if (!alert) {
        return;
      }
      growl.error('Alert: ' + alert.description);
      var dataObject = {};
      alert.value.fields.forEach(function(field) {
        var name = field.name;
        var value = field.value;
        if ((typeof value === 'string') && (value.indexOf('$') === 0)) {
          var refName = alert.deviceModel + '.' + value.substr(1);
          value = $scope.device[refName];
        }
        dataObject[name] = value;
      });
      var alertMessage = {
        urn: alert.urn,
        modelURN: alert.deviceModel,
        data: dataObject
      };
      if (alert.image) {
        /* jshint shadow:true */
        for (var i = 0; i < $scope.device.dm.images.length; i++) {
          if ($scope.device.dm.images[i].name === alert.image) {
            $scope.device.alertImage = $scope.device.dm.images[i];
          }
        }

        if ($scope.device.alertImage) {
          $scope.oldImage = $scope.device.stateImage;
          $scope.alertImageInEffect = true;

          $scope.device.stateImage = $scope.device.alertImage;
          var effectTime = alert.imageTime || 2000;
          window.setTimeout(function() {
            $scope.device.stateImage = $scope.oldImage;
            $scope.alertImageInEffect = false;
          }, effectTime);
        }
      }
      $http.post('/sim/devices/' + $scope.device.id + '/alerts', alertMessage);
