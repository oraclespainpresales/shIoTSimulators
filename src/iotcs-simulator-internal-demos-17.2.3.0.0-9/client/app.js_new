(function() {
  'use strict';

  var mod = angular.module('simulator');

  mod.controller('EaCtrl', ["$scope", "$log", "$rootScope", "$filter", "$interval", "$http", "growl", function($scope, $log, $rootScope, $filter, $interval, $http, growl) {

    $scope.checkInProgress = true;

    $scope.isInitialized = function() {
      if (!$rootScope.simulatorGlobals ||
        ($rootScope.simulatorGlobals.ea === undefined) ||
        ($rootScope.simulatorGlobals.dm === undefined)) {
        // not yet initialized
        $scope.checkInProgress = false;
        return undefined;
      }
      // initialization terminated
      return ($rootScope.simulatorGlobals.ea !== null && $rootScope.simulatorGlobals.dm !== null);
    };

    // Create the 3 arrays of tabs we have, the currently showing ones '$scope.tabs'
    // the '$scope.eaTabs' for when in enterprise app mode and the '$scope.simTabs'
    // for when we are in simulator mode. The enterprise app tab's routes were already
    // created in the config method. The simTabs will be augmented by adding / removing
    // device tabs as individual devices are found or created. When we switch modes
    // '$scope.tabs' it switched to = one of the other sets of tabs
    $scope.tabs = [
      {heading: 'Dashboard', route: 'ea.dashboard'},
      {heading: 'Device Control', route: 'ea.controls'}
    ];

    if ($rootScope.simulatorGlobals) {
      return;
    }
    $rootScope.simulatorGlobals = {
      ea: undefined,
      dm: undefined,
      deviceList: {}
    };

    // We are using test server: we need to set auth.activated (because no redirect for authentication)
    iotcs.oracle.iot.client.test.auth.activated = (typeof $rootScope.config.server.test === 'undefined') ? false :
      $rootScope.config.server.test;
    iotcs.oracle.iot.tam.store = '/iotcs-cl/trustedAssetsStore.json';
    iotcs.oracle.iot.tam.storePassword = 'changeit';

    $scope.update = function() {

      var app = $rootScope.config.simulator.iotappname;
      $scope.checkInProgress = true;
      // If newClient() won't call callback function after 4 seconds, switch iotcs.oracle.iot.client.test.auth.activated flag for next attempt
      $scope.authSwitcherTimeout = setTimeout(function() {
        $scope.checkInProgress = false;
        iotcs.oracle.iot.client.test.auth.activated = !iotcs.oracle.iot.client.test.auth.activated;
      }, 4000);
      iotcs.enterprise.EnterpriseClient.newClient(app, function(client, exception) {
        // iotcs.oracle.iot.client.test.auth.activated flag is fine, no need to switch it
        clearTimeout($scope.authSwitcherTimeout);
        if (exception) {
          $rootScope.simulatorGlobals.ea = null;
          $rootScope.simulatorGlobals.dm = null;
          $scope.checkInProgress = false;
          $log.log('cannot create client: ' + JSON.stringify(exception));
          growl.error('Cannot create EnterpriseClient:' + exception.message);
        } else {
          var dmList = [];
          client.getDeviceModels().page('first', 200).then(function(response) {
            if (response.items) {
              response.items.forEach(function(model) {
                dmList.push(model);
              });
            }
            $rootScope.simulatorGlobals.ea = client;
            $rootScope.simulatorGlobals.dm = dmList;
            $rootScope.$digest();
          });
          $scope.checkInProgress = false;
        }
      });
    };

    $scope.update();

    var handler = $interval(checkAppCreated, 5000);

    function checkAppCreated() {
      if ($rootScope.simulatorGlobals.ea && $rootScope.simulatorGlobals.dm) {
        if (handler) {
          $interval.cancel(handler);
        }
      } else if (!$scope.checkInProgress) {
        $scope.update();
      }
    }
  }]);

}());

