(function() {
  'use strict';

  var mod = angular.module('simulator');

  mod.controller('ControlsCtrl', ["$scope", "$log", "$rootScope", "$timeout", "growl", function($scope, $log, $rootScope, $timeout, growl) {

    $scope.selectedDevice = {
      devicespec: {'id': 'loading...'},
      virtualdevice: {},
      fake: true,
      firstUpdate: false
    };

    $scope.updateInProgress = false;

    $scope.$on('device.list.updated', function() {
      var selection = $scope.selectedDevice;
      $scope.updateInProgress = false;
      if (!selection || selection.fake) {
        var list = $rootScope.simulatorGlobals.deviceList;
        var firstkey = Object.keys(list)[0];
        $scope.setDevice(list[firstkey]);
      }
      $scope.$apply();
    });

    $scope.updateDeviceList = function() {
      $scope.appFound = $scope.$parent.isInitialized();
      if (!$scope.appFound || $scope.updateInProgress) {
        // IoT application not found or still waiting for response
        return;
      }

      var newList = {};
      $scope.dmCount = $rootScope.simulatorGlobals.dm.length;
      var responsesCount = 0;
      $scope.updateInProgress = true;
      for (var deviceModel in $rootScope.simulatorGlobals.dm) {
        if (!$rootScope.simulatorGlobals.dm.hasOwnProperty(deviceModel)) {
          continue;
        }
        (function() {
          var dm = $rootScope.simulatorGlobals.dm[deviceModel];
          $rootScope.simulatorGlobals.ea.getActiveDevices(dm.urn).page('first', 200).then(function(response) {
            responsesCount++;
            if (response.items && response.items.length > 0) {
              var itemsCount = 0;
              response.items.forEach(function(item) {
                var device = $rootScope.simulatorGlobals.deviceList[item.id];
                // Show only devices active in the simulator
                var simDevice = $rootScope.devices.find(function(dev) {
                  return dev.id === item.id;
                });
                if (!device && simDevice && item.state !== 'DECOMMISSIONED') {
                  // new device: create associated virtual device instance
                  var vd = $rootScope.simulatorGlobals.ea.createVirtualDevice(item.id, dm);
                  device = {
                    devicespec: item,
                    virtualdevice: vd
                  };
                  device.devicespec.ownDeviceModels = device.devicespec.deviceModels.filter(function(dm) {
                    return dm.system === false;
                  });
                  // We need to know the name of the simulated device, not the device model name
                  device.deviceName = simDevice.deviceName;
                }
                if (device) {
                  newList[item.id] = device;
                }
                itemsCount++;
                // Making sure that all items received and processed before updating the list
                if ((responsesCount === $scope.dmCount) && (itemsCount === response.items.length)) {
                  $rootScope.simulatorGlobals.deviceList = newList;
                  $scope.$emit('device.list.updated');
                }
              });
            } else {
              if (responsesCount === $scope.dmCount) {
                $rootScope.simulatorGlobals.deviceList = newList;
                $scope.$emit('device.list.updated');
              }
            }
          });
        }());
      }
    };

    $scope.actionHandler = function(virtualDevice, actionName, value) {
      virtualDevice.call(actionName, value);
      growl.success('Action "' + actionName + (typeof value !== 'undefined' ? '{value: ' + String(value) + '}' : '') +
        '" sent to device');
    };

    $scope.refreshSlider = function() {
      $timeout(function() {
        $scope.$broadcast('rzSliderForceRender');
      });
    };

    $scope.setDevice = function(device) {
      if (device && (device.fake !== null)) {
        // $log.log('device(' + device.devicespec.id + ')');
        if ($scope.selectedDevice === device) {
          return;
        }
        $scope.selectedDevice = device;
        $scope.attrs = device.devicespec.ownDeviceModels[0].attributes;
        $scope.attrs.forEach(function(attr) {
          if (attr.writable && ((attr.type === 'NUMBER') || (attr.type === 'INTEGER'))) {
            attr.slider = {};
            attr.slider.bufferValue = $scope.selectedDevice.virtualdevice[attr.name].value;
            Object.defineProperty(attr.slider, 'value', {
              get: function() {
                return attr.slider.bufferValue || $scope.selectedDevice.virtualdevice[attr.name].value;
              },
              set: function(newValue) {
                attr.slider.bufferValue = newValue;
              },
              enumerable: true
            });
            var range = attr.range.split(',');
            attr.slider.options = {
              floor: range[0],
              ceil: range[1]
            };
            attr.slider.onChange = function() {
              // do nothing
            };
            attr.slider.onEnd = function() {
              $scope.selectedDevice.virtualdevice[attr.name].value = attr.slider.bufferValue;
            };
            $scope.refreshSlider();
          }
        });
        device.devicespec.ownDeviceModels[0].actions.forEach(function(action) {
          $scope.selectedDevice[action.name] = $scope.actionHandler.bind(null, $scope.selectedDevice.virtualdevice,
            action.name);
        });

      } else {
        $scope.selectedDevice = {
          devicespec: {id: 'no active device found'},
          virtualdevice: {},
          fake: true
        };
      }
      // $scope.$digest();
    };

    $rootScope.$watch('simulatorGlobals.dm.length', function() {
      if ($rootScope.simulatorGlobals.dm && $rootScope.simulatorGlobals.dm.length > 0) {
        $scope.updateDeviceList();
      }
    });

  }]);

}());

